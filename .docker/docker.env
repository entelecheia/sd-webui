SDW_GITHUB_REPO="stable-diffusion-webui"
SDW_GITHUB_BRANCH="sd-webui-docker"
SDW_SERVER_NAME=
SDW_CUDA_DEVICE_ID="0"
SDW_HOST_SSH_PORT="2722"
