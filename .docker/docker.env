IMAGE_TAG="${VERSION}-${VARIANT}"
IMAGE_NAME="${DOCKER_USERNAME}/${DOCKER_NAME}:${IMAGE_TAG}"
BUILD_FROM="nvcr.io/nvidia/pytorch:23.04-py3"
